  height: _height,
  rotation = 0,
  fill,
  isSelected,
  selectionColor: _selectionColor,
  fontFamily = 'Inter, sans-serif',
  fontSize = 20,
  fontWeight = 'normal',
  fontStyle = 'normal',
  textAlign = 'left',
  textDecoration = '',
  onSelect,
  onDragStart,
  onDragEnd,
  onTransformEnd,
  onDoubleClick,
}: TextShapeProps) {
  const shapeRef = useRef<Konva.Text>(null)
  const trRef = useRef<Konva.Transformer>(null)

