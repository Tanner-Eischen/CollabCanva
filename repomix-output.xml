  DEFAULT_FAR_BACKGROUND_LAYER,
  DEFAULT_BACKGROUND_LAYER,
  DEFAULT_GROUND_LAYER,
  DEFAULT_DECALS_LAYER,
  DEFAULT_PROPS_LAYER,
  DEFAULT_COLLISION_LAYER,
  DEFAULT_FOREGROUND_LAYER,
]

/**
 * Platform preset - optimized for platformer games
 */
